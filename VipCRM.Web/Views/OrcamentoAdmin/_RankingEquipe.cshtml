<div class=" box box-primary" ng-init="rankingEquipe()">
    <div class="box-header">
        <h3 class="box-title"><b>Ranking X Equipes</b><small> (Mês atual)</small></h3>
    </div>
    <div class="box-body">
        <table id="monitores14" class="table table-striped table-hover" ng-show="rankingEquipes.length > 0">
            <thead>
                <tr>
                    <th>Equipe</th>
                    <th>Qtde.</th>
                    <th>Valor</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>

                <tr ng-repeat="item in rankingEquipes">

                    <td><b>{{item.Equipe}}</b></td>
                    <td><b>{{item.Quantidade}}</b></td>
                    <td><b>{{item.Valor}}</b></td>
                    <td>
                        <div class="progress progress-xs">
                            <div class="progress-bar progress-bar-green" ng-style="{width: Percentual(item.Percentual) }"></div>

                        </div>
                        {{item.Percentual}}%
                    </td>
                </tr>

            </tbody>
        </table>
    </div><!-- /.box-body -->

</div>