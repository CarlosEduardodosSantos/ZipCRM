

<div class=" box box-primary">
    <div class="box-header">
        <i class="ion ion-clipboard"></i>
        <h3 class="box-title">Ocorrências em Encaminhadas</h3>
    </div>
    <div class="box-body">
        <table id="monitores1" class="table table-striped table-hover" ng-show="Monitores.length > 0">
            <thead>
                <tr>
                    <th>Programador</th>
                    <th>Ocorrencia(s)</th>
                    <th>Executando</th>
                    <th>Concluida(s)</th>
                    <th>Pendencia</th>
                    <th>Evolução</th>
                    <th></th>
                    <th>Atualização</th>
                    <th style="width: 40px"></th>
                </tr>
            </thead>
            <tbody id="monitores2">

                <tr ng-repeat="item in Monitores">

                    <td>{{item.UsuarioNome}}</td>
                    <td ng-click="QualMeuIndex($index, null)" data-toggle="modal" data-target="#myModal"><a href="#">{{item.QtdeOcorrencia}}</a></td>
                    <td ng-click="QualMeuIndex($index, 'Execução')" data-toggle="modal" data-target="#myModal"><a href="#">{{item.QtdeOcorrenciaExecucao}}</a></td>
                    <td ng-click="QualMeuIndex($index, 'Concluido')" data-toggle="modal" data-target="#myModal"><a href="#">{{item.QtdeOcorrenciaConcluida}}</a></td>
                    <td ng-click="QualMeuIndex($index, 'Pendente')" data-toggle="modal" data-target="#myModal"><a href="#">{{item.QtdeOcorrenciaPendente}}</a></td>
                    <td>
                        <div class="progress progress-xs">
                            <div class="progress-bar progress-bar-green" ng-style="{width: percentualRoteiro(item.QtdeOcorrenciaConcluida,item.QtdeOcorrencia) }"></div>
                        </div>
                    </td>
                    <td><span class="badge bg-red"> {{percentualRoteiro(item.QtdeOcorrenciaConcluida,item.QtdeOcorrencia)}}</span></td>
                    <td>{{jsonTime(item.DataUltAtualizacao)}}</td>
                </tr>

            </tbody>
        </table>
        <div class="clearfix">
            <span class="pull-left">Geral</span>
            <small class="pull-right" data-bind="text:  PercentualGeral()"></small>
        </div>
        <div class="progress xs">
            <div class="progress-bar progress-bar-green" data-bind="style: { width: PercentualGeral() }"></div>
        </div>
    </div><!-- /.box-body -->

    <div id="myModal" class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Pendência(s) de {{Tecnico.UsuarioNome}}</h4>
                </div>

                <table class="table table-striped table-hover" ng-show="Tecnico.TecnicoClienteItem.length > 0">
                    <thead>
                    <tr>
                        <th>Data</th>
                        <th>Cliente</th>
                        <th>Inicio</th>
                        <th>Fim</th>
                        <th>Situação</th>
                        <th>Veiculo</th>
                    </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="tec in Tecnico.TecnicoClienteItem">
                            <td>{{ConvertDateTime(tec.DataAgendamento)}}</td>
                            <td>{{tec.ClienteNome}}</td>
                            <td class="text-yellow">{{jsonTime(tec.DataInicioVip)}}</td>
                            <td class="text-green">{{jsonTime(tec.DataFimVip)}}</td>
                            <td>{{tec.Situacao}}</td>
                            <td>{{tec.Veiculo}}</td>
                        </tr>
                    </tbody>
                </table>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Fechar</button>
                </div>
            </div>
        </div>
    </div>

</div>