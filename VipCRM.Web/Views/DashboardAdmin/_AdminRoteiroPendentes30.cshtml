<div class=" box box-primary">
    <div class="box-header">
        <!--<i class="ion ion-clipboard"></i>-->
        <h3 class="box-title">Ranking Mensal (AC)</h3>
    </div>
    <div class="box-body">
        <table id="monitores11" class="table table-striped table-hover" ng-show="MonitoresPendentes30.length = 5">
            <!---->
            <thead>
                <tr>
                    <th>Técnico</th>
                    <th>Oc.Abertas</th>
                </tr>
            </thead>
            <tbody id="monitores22">

                <tr ng-repeat="item in MonitoresPendentes30">

                    <td><b>{{item.UsuarioNome}}</b></td>
                    <td ng-click="QualMeuIndex($index, 'Pendente', 2)" data-toggle="modal" data-target="#myModal"><a href="#">{{item.QtdeOcorrenciaPendente30}}</a></td>

                </tr>

            </tbody>
        </table>
        <br />
        <!-- Totalizador -->
        <div class="box-body">
            <table id="monitores1" class="table table-striped table-hover" ng-show="Totalizadores.length = 1">
                <thead>
                    <tr>
                        <th>Total no Mês</th>
                        <td><a href="#">{{Totalizador.OcorrenciaPendentesTotal}}</a></td>
                    </tr>
                </thead>

            </table>
        </div>
        <!-- FIM Totalizador -->
        <br />
        <button ng-click="" data-toggle="modal" data-target="#myModal1" class="btn-instagram">Mês Anterior</button>

        <!--div class="progress xs">
        <div class="progress-bar progress-bar-green" data-bind="style: { width: PercentualGeral() }"></div>
    </div>-->
    </div><!-- /.box-body -->

    <div id="myModal" class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Pendência(s) de {{Tecnico.UsuarioNome}}</h4>
                </div>

                <table class="table table-striped table-hover" ng-show="Tecnico.TecnicoClienteItem.length > 0">
                    <thead>
                        <tr>
                            <th>Data</th>
                            <th>Cliente</th>
                            <th>Inicio</th>
                            <th>Fim</th>
                            <th>Situação</th>
                            <th>Veiculo</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="tec in Tecnico.TecnicoClienteItem">
                            <td>{{ConvertDateTime(tec.DataAgendamento)}}</td>
                            <td>{{tec.ClienteNome}}</td>
                            <td class="text-yellow">{{jsonTime(tec.DataInicioVip)}}</td>
                            <td class="text-green">{{jsonTime(tec.DataFimVip)}}</td>
                            <td>{{tec.Situacao}}</td>
                            <td>{{tec.Veiculo}}</td>
                        </tr>
                    </tbody>
                </table>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Fechar</button>
                </div>
            </div>
        </div>
    </div>

    <div id="myModal1" class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Ranking Mensal (AC) Mês Anterior</h4>
                </div>

                <table class="table table-striped table-hover" ng-show="MonitoresPendentes30Anterior.length > 0">
                    <thead>
                        <tr>
                            <th>Técnico</th>
                            <th>Oc.Abertas</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="item in MonitoresPendentes30Anterior">
                            <td><b>{{item.UsuarioNome}}</b></td>
                            <td><b>{{item.QtdeOcorrenciaPendente30Anterior}}</b></td>
                        </tr>
                    </tbody>
                </table>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Fechar</button>
                </div>
            </div>
        </div>
    </div>

</div>