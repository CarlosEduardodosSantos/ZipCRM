

<div class=" box box-primary">
    <div class="box-header">
   
        <h3 class="box-title"><b>Monitoramento de Roteiro</b></h3>
    </div>
    <div class="box-body">
        <table id="monitores1" class="table table-striped table-hover" ng-show="Monitores.length > 0">
            <thead>
                <tr>
                    <th>Téc.</th>
                    <th>Oc.</th>
                    <th>Exec.</th>
                    <th>Conc.</th>
                    <th>Pend.</th>
                    <th>Veículo</th>
                    <th></th>
                    <th>Atu.</th>
                    <th style="width: 40px"></th>
                </tr>
            </thead>
            <tbody id="monitores2">

                <tr ng-repeat="item in Monitores">

                    <td><b>{{item.UsuarioNome}}</b></td>
                    <td ng-click="QualMeuIndex($index, null,1)" data-toggle="modal" data-target="#myModal"><a href="#">{{item.QtdeOcorrencia}}</a></td>
                    <td ng-click="QualMeuIndex($index, 'Execução',1)" data-toggle="modal" data-target="#myModal"><a href="#">{{item.QtdeOcorrenciaExecucao}}</a></td>
                    <td ng-click="QualMeuIndex($index, 'Concluido',1)" data-toggle="modal" data-target="#myModal"><a href="#">{{item.QtdeOcorrenciaConcluida}}</a></td>
                    <td ng-click="QualMeuIndex($index, 'Pendente',1)" data-toggle="modal" data-target="#myModal"><a href="#">{{item.QtdeOcorrenciaPendente}}</a></td>
                    <td>{{item.UsuarioVeiculo}}
                        <!--<div class="progress progress-xs">
                            <div class="progress-bar progress-bar-green" ng-style="{width: percentualRoteiro(item.QtdeOcorrenciaConcluida,item.QtdeOcorrencia) }"></div>
                        </div>-->
                    </td>
                    <td><!--<span class="badge bg-red"> {{percentualRoteiro(item.QtdeOcorrenciaConcluida,item.QtdeOcorrencia)}}</span>--></td>
                    <td>{{jsonTime(item.DataUltAtualizacao)}}</td>
                </tr>

            </tbody>
        </table>
        <div class="clearfix">
            <span class="pull-left">Geral</span>
            <small class="pull-right" data-bind="text:  PercentualGeral()"></small>
        </div>
        <div class="progress xs">
            <div class="progress-bar progress-bar-green" data-bind="style: { width: PercentualGeral() }"></div>
        </div>
    </div><!-- /.box-body -->

    <div id="myModal" class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Pendência(s) de {{Tecnico.UsuarioNome}}</h4>
                </div>

                <table class="table table-striped table-hover" ng-show="Tecnico.TecnicoClienteItem.length > 0">
                    <thead>
                    <tr>
                        <th>Data</th>
                        <th>Cliente</th>
                        <th>Inicio</th>
                        <th>Fim</th>
                        <th>Situação</th>
                        <th>Veiculo</th>
                    </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="tec in Tecnico.TecnicoClienteItem">
                            <td>{{ConvertDateTime(tec.DataAgendamento)}}</td>
                            <td>{{tec.ClienteNome}}</td>
                            <td class="text-yellow">{{jsonTime(tec.DataInicioVip)}}</td>
                            <td class="text-green">{{jsonTime(tec.DataFimVip)}}</td>
                            <td>{{tec.Situacao}}</td>
                            <td>{{tec.Veiculo}}</td>
                        </tr>
                    </tbody>
                </table>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Fechar</button>
                </div>
            </div>
        </div>
    </div>

</div>