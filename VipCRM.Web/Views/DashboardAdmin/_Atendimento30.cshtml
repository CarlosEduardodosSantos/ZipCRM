<!--MONITORAMENTO DE PRODUTIVIDADE<i class="ion ion-clipboard"></i>-->
<div class=" box box-primary">
    <div class="box-header">

        <h3 class="box-title">Ranking Mensal (PD)</h3>
    </div>
    <div class="box-body">
        <table id="monitores1" class="table table-striped table-hover" ng-show="MonitoresConcluido30.length = 3">
            <thead>
                <tr>
                    <th>Técnico</th>
                    <th>Oc. Finalizadas</th>
                </tr>
            </thead>
            <tbody id="monitores2">

                <tr ng-repeat="item in MonitoresConcluido30">

                    <td><b>{{item.UsuarioNome}}</b></td>
                    <td><a href="#">{{item.QtdeConcluida30}}</a></td>

                </tr>

            </tbody>
        </table>
        <br />
        <!-- Totalizador -->
        <div class="box-body">
            <table id="monitores1" class="table table-striped table-hover" ng-show="Totalizadores.length = 1">
                <thead>
                    <tr>
                        <th>Total</th>
                        <td><a href="#">{{Totalizador.OcorrenciaConcluidasTotal}}</a></td>
                    </tr>
                </thead>

            </table>
        </div>
        <!-- FIM Totalizador -->
        <br />
        <button ng-click="" data-toggle="modal" data-target="#myModal3" class="btn-instagram">Mês Anterior</button>
        <br />

        <!--<div class="progress xs">
        <div class="progress-bar progress-bar-green" data-bind="style: { width: PercentualGeral() }"></div>
    </div>-->
    </div><!-- /.box-body -->
    
    <div id="myModal" class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Pendência(s) de {{Tecnico.UsuarioNome}}</h4>
                </div>

                <table class="table table-striped table-hover" ng-show="Tecnico.TecnicoClienteItem.length > 0">
                    <thead>
                        <tr>
                            <th>Data</th>
                            <th>Cliente</th>
                            <th>Inicio</th>
                            <th>Fim</th>
                            <th>Situação</th>
                            <th>Veiculo</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="tec in Tecnico.TecnicoClienteItem">
                            <td>{{ConvertDateTime(tec.DataAgendamento)}}</td>
                            <td>{{tec.ClienteNome}}</td>
                            <td class="text-yellow">{{jsonTime(tec.DataInicioVip)}}</td>
                            <td class="text-green">{{jsonTime(tec.DataFimVip)}}</td>
                            <td>{{tec.Situacao}}</td>
                            <td>{{tec.Veiculo}}</td>
                        </tr>
                    </tbody>
                </table>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Fechar</button>
                </div>
            </div>
        </div>
    </div>

    <div id="myModal3" class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Ranking Mensal (PD) Mês Anterior</h4>
                </div>

                <table class="table table-striped table-hover" ng-show="MonitoresConcluido30Anterior.length > 0">
                    <thead>
                        <tr>
                            <th>Técnico</th>
                            <th>Oc.Finalizadas</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="item in MonitoresConcluido30Anterior">
                            <td><b>{{item.UsuarioNome}}</b></td>
                            <td><b>{{item.QtdeConcluida30Anterior}}</b></td>
                        </tr>
                    </tbody>
                </table>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Fechar</button>
                </div>
            </div>
        </div>
    </div>

</div>
